{
  "name": "gha-matrix-tester",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "go run . -dev -watch",
    "build": "go run .",
    "css:build": "postcss src/index.css",
    "css:watch": "postcss src/index.css -w",
    "format": "npx @biomejs/biome format ./src",
    "format:fix": "npx @biomejs/biome format --write ./src",
    "lint": "npx @biomejs/biome lint ./src",
    "typecheck": "tsc --noEmit",
    "vet": "npm run lint && npm run typecheck && npm run format",
    "vet:ci": "npm run typecheck && npx @biomejs/biome ci",
    "test": "echo \"no test specified\""
  },
  "license": "Mozilla Public License 2.0",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "esbuild": "0.21.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  }
}
