		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@fortawesome/react-fontawesome": "^0.2.3",
		"@uiw/react-textarea-code-editor": "^3.1.1",
		"json-stringify-deterministic": "^1.0.12",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"yaml": "^2.8.2",
		"yup": "^1.7.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
